.select{
    width: 90%;
    height: 70rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    color: rgb(172, 172, 172);
    background-color: #fff;
    margin-top: 30rpx;
    border-radius:15rpx;
}
.actList{
    background-color: #fff;
    width: 85%;
    padding: 25rpx;
    margin-top: 30rpx;
    border-radius:15rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.actList_top{
    width: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 5rpx;
}
.act_txt{
    width: 65%;
    height: 80%;
    display: flex;
    flex-direction: column;
}
.actList_down{
    width: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-top: 20rpx;
    word-break: break-all;
    color: rgb(148, 148, 148);
}