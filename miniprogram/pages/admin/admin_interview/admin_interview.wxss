
.select{
    width: 100%;
    height: 150rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background-color: #fff;
}
.select_1{
    width: 65%;
    height: 50%;
    border-radius: 15rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    background-color: #f5f4f4;
    color: rgb(129, 129, 129);
}
.title{
    margin-top: 25rpx;
    width: 100%;
    height: 50rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    padding-top: 1%;
    border-radius: 25rpx 25rpx 0 0;
    border-top: 1px solid rgb(209, 209, 209);
    font-size: 32rpx;
    font-weight: bolder;
    background-color: #fff;
}
.fxk .wx-checkbox-input{
    border-radius: 50%;
}
.actname{
    float:left;
    padding-left: 15rpx; 
    width: 35%;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis; 
  
}
.phone{
    font-size: 30rpx;
    width: 33%;
    color: rgb(253, 181, 48);
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-left:5%;
}
.pop_up{
    width: 40%;
    height: 70%;
    background-color: white;
    display: flex;
    flex-direction: column;
    border-radius: 15rpx;
    border: 1rpx solid #f3f3f3;
    opacity: 0.95;

}
.pop_up_txt{
    width: 90%;
    height: 20%;
    display: flex;
    align-items: center;
    padding-left: 5%;
}
.pop_up_title{
    font-weight: bolder;
    font-size: 32rpx;
}
.pop_up_info{
    padding-left: 3%;
    color: rgb(63, 63, 63);
}